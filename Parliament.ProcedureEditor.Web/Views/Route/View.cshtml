@model int

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<input id="routeId" type="hidden" value="@Model" />
<section>
    <div class="container">
        <h2>Procedure: <span data-bind="text: route().ProcedureName"></span></h2>
        <h3 data-bind="text: route().FromProcedureStepName"></h3>
        <ul class="list-inline" data-bind="foreach: route().FromProcedureStepHouseNames">
            <li class="list-inline-item"><h4><small data-bind="text: $data"></small></h4></li>
        </ul>
        <h3 data-bind="text: route().ProcedureRouteTypeName"></h3>
        <h3 data-bind="text: route().ToProcedureStepName"></h3>
        <ul class="list-inline" data-bind="foreach: route().ToProcedureStepHouseNames">
            <li class="list-inline-item"><h4><small data-bind="text: $data"></small></h4></li>
        </ul>
        <h3>Id: <span data-bind="text: route().TripleStoreId"></span></h3>
        <a href="/Route/Edit/@Model" class="btn btn-outline-primary btn-sm">Edit</a>
    </div>
</section>

@section scripts{
<script data-main="/Scripts/Views/routeview.js" src="~/Scripts/require.js"></script>
}
